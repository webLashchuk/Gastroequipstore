{
  "svguse": {
    "scope": "html",
    "prefix": "s-use",
    "body": [
      "<svg class=\"\">",
      "  <use xlink:href=\"./$2\"></use>",
      "</svg>"
    ],
    "description": "Snippet for use svg sprites"
  },
  "burger": {
    "scope": "html",
    "prefix": "s-burger",
    "body": [
      "<button class=\"burger\" aria-label=\"#\" data-burger type=\"button\">",
      "<svg class=\"burger-icon\" viewBox=\"0 0 100 100\" width=\"80\">",
      "<path class=\"line top\" d=\"m 70,33 h -40 c 0,0 -8.5,-0.149796 -8.5,8.5 0,8.649796 8.5,8.5 8.5,8.5 h 20 v -20\" />",
      "<path class=\"line middle\" d=\"m 70,50 h -40\" />",
      "<path class=\"line bottom\" d=\"m 30,67 h 40 c 0,0 8.5,0.149796 8.5,-8.5 0,-8.649796 -8.5,-8.5 -8.5,-8.5 h -20 v 20\" />",
      "</svg>",
      "</button>"
    ],
    "description": "Snippet for burger btn"
  },
  "checkbox": {
    "scope": "html",
    "prefix": "s-checkbox",
    "body": [
      "<label class=\"checkbox\">",
      "  <input class=\"checkbox__field\"> type=\"checkbox\"",
      "  <span class=\"checkbox__content\">$1</span>",
      "</label>"
    ],
    "description": "Snippet for custom checkbox"
  },
  "navigation": {
    "scope": "html",
    "prefix": "s-nav",
    "body": [
      "<nav class=\"nav\" title=\"$1\">",
      "  <ul class=\"nav__list\">",
      "    <li class=\"nav__item\"><a class=\"nav__link\" href=\"#\"></a></li>",
      "  </ul>",
      "</nav>"
    ],
    "description": "Snippet for navigation"
  },
  "social": {
    "scope": "html",
    "prefix": "s-social",
    "body": [
      "<ul class=\"social\" title=\"$1\">",
      "  <li class=\"social__item\"><a class=\"social__link social__link--fb\" href=\"#\" target=\"_blank\"></a></li>",
      "</ul>"
    ],
    "description": "Snippet for social links"
  },
  "basicform": {
    "prefix": "s-form",
    "body": [
      "<form class=\"form\" action=\"#\">",
      "  <label class=\"form__label\">",
      "    <input class=\"input-reset form__input\" type=\"text\" name=\"name\" placeholder=\"placeholder\">",
      "  </label>",
      "  <button class=\"form__btn\" type=\"submit\">Text</button>",
      "</form>"
    ],
    "description": "Snippet for basic form"
  },
  "basicswiper": {
    "prefix": "s-swiper",
    "body": [
      "<div class=\"swiper $1\">",
      "  <div class=\"swiper-wrapper\">",
      "     <div class=\"swiper-slide\">",
      "     </div>",
      "  </div>",
      "</div>"
    ],
    "description": "Snippet for basic swiper structure"
  },
  "openModalBtn": {
    "prefix": "s-modalOpen",
    "body": [
      "<button class=\"$1\" aria-label=\"Open modal\" data-modal-open=\"$2\">$3</button>"
    ],
    "description": "Snippet for modal btn open"
  },
  "modal": {
    "prefix": "s-modal",
    "body": [
      "<dialog class=\"dialog\">",
      " <button class=\"dialog__close\" aria-label=\"Close modal\" data-modal-close>$3</button>",
      " <div class=\"dialog__content\">$2</div>",
      "</dialog>"
    ],
    "description": "Snippet for modal basic structure"
  },
  "picture": {
    "prefix": "s-picture",
    "body": [
      "<picture>",
      "  <source srcset=\"$1.webp?as=webp\" type=\"image/webp\">",
      "  <img class=\"image\" src=\"$1\" width=\"$4\" height=\"$5\" alt=\"$3\" loading=\"lazy\" >",
      "</picture>"
    ],
    "description": "Snippet for basic logo structure"
  },
  "svgimg": {
    "scope": "html",
    "prefix": "s-svgimg",
    "body": [
      "<img class=\"image\" src=\"$1.$2\" width=\"$4\" height=\"$5\" alt=\"$3\" loading=\"lazy\">"
    ],
    "description": "Snippet for simple html svg images"
  },
  "tabs html sctructure": {
    "prefix": "s-tabs",
    "body": [
      "<div class=\"tabs\" data-tabs=\"$1\">",
      "  <ul class=\"tabs__switcher\">",
      "    <li class=\"tabs__item\"><button class=\"tabs__btn\" type=\"button\">$2</button></li>",
      "    <li class=\"tabs__item\"><button class=\"tabs__btn\" type=\"button\">$3</button></li>",
      "    <li class=\"tabs__item\"><button class=\"tabs__btn\" type=\"button\">$4</button></li>",
      "  </ul>",
      "  <div class=\"tabs__content\">",
      "    <div class=\"tabs__panel\">$5</div>",
      "    <div class=\"tabs__panel\">$6</div>",
      "    <div class=\"tabs__panel\">$7</div>",
      "  </div>",
      "</div>"
    ],
    "description": "Snippet for basic tabs structure"
  },
  "accordion": {
    "prefix": "s-accordion",
    "body": [
      "<ul class=\"accordion\">",
      " <li class=\"accordion__item\">",
      "   <details class=\"accordion__content\">",
      "     <summary class=\"accordion__summary\">$1</summary>",
      "       <div class=\"accordion__content\">$2</div>",
      "   </details>",
      " </li>",
      "</ul>"
    ],
    "description": "Snippet for simple html svg images"
  }
}
